{
  "name": "server",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "clean": "rimraf dist",
    "copy-deps": "copy-newer --cwd node_modules ** ../dist/node_modules",
    "start": "node ../dist/server/server.js",
    "preserve" : "npm run build",
    "serve": "concurrently \"npm run build.w\" \"nodemon ../dist/server/server.js\"",
    "lint": "tslint \"src/**/*.ts\"",
    "prebuild": "npm install && rimraf ../dist/public",
    "build": "npm run copy-deps && tsc --p ./src",
    "build.w": "npm run copy-deps && tsc --p ./src -w"
  },
  "private": true,
  "dependencies": {
    "body-parser": "1.17.1",
    "cors": "2.8.3",
    "express": "4.15.2",
    "inversify": "2.0.1",
    "inversify-express-utils": "2.0.2",
    "mongodb": "2.2.25",
    "morgan": "1.8.1",
    "reflect-metadata": "0.1.10"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/cors": "2.8.1",
    "@types/express": "4.0.35",
    "@types/mongodb": "2.1.43",
    "@types/morgan": "1.7.32",
    "@types/node": "7.0.12",
    "concurrently": "3.4.0",
    "copy-newer": "2.1.2",
    "morgan": "1.8.1",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "tslint": "5.1.0",
    "typescript": "2.2.2"
  }
}
